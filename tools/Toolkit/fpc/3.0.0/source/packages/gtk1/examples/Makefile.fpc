#
#   Makefile.fpc for Free Pascal GTK 1.2.x Examples
#

[target]
dirs=tutorial
programs=entry notebook scribble clist ttt_test pixmap list progressbar filesel \
         statusbar toolbar rulers spinbutton gtkgldemo

[require]
packages=gtk1

[clean]
units=tictactoe

[install]
fpcpackage=y

[default]
fpcdir=../../..

[rules]
.NOTPARALLEL:
