#
#   Makefile.fpc for FCL FPCUnit test framework.
#

[package]
main=fcl

[target]
units=asserttest frameworktest suitetest

[require]
packages=fcl-base fcl-xml fcl-fpcunit

[compiler]
options=-S2h
#targetdir=../../$(OS_TARGET)

[install]
fpcpackage=y

[default]
fpcdir=../../../..

[rules]
.NOTPARALLEL:
