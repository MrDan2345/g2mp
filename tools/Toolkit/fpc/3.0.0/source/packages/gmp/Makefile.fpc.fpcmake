#
#   Makefile.fpc for Free Pascal ImLib 1.x Packages
#

[package]
name=gmp 
version=3.0.0rc2

[target]
units=gmp
exampledirs=examples

[require]
libc=y

[compiler]
includedir=src
sourcedir=src 


[install]
fpcpackage=y
fpcsubdir=packages

[default]
fpcdir=../..

[shared]
build=n

[rules]
.NOTPARALLEL:
