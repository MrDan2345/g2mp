#
#   Makefile.fpc for Postgres bindings
#

[package]
name=postgres
version=3.0.0rc2

[target]
units=dllist dllistdyn postgres postgres3 postgres3dyn
exampledirs=examples

[require]
libc=y

[install]
fpcpackage=y

[default]
fpcdir=../..

[compiler]
includedir=src
sourcedir=src

[shared]
build=n

[rules]
.NOTPARALLEL:

